{
  "name": "nextjs-starter",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "export": "next export",
    "start": "next start",
    "deduplicate": "npx yarn-deduplicate && yarn install",
    "lint": "next lint",
